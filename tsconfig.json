{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "strictNullChecks": true,
    "strict": true,
    "alwaysStrict": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "pretty": true,
    "listFiles": true,
    "listEmittedFiles": true,
    "typeRoots": ["./node_modules/@types"]
  },
  "exclude": ["node_modules", "**/*.spec.ts", "*.js"]
}
